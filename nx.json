{
  "npmScope": "nxext",
  "workspaceLayout": {
    "appsDir": "e2e",
    "libsDir": "packages"
  },
  "implicitDependencies": {
    "workspace.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tslint.json": "*",
    "nx.json": "*",
    "tsconfig.base.json": "*"
  },
  "projects": {
    "stencil": {
      "tags": []
    },
    "stencil-e2e": {
      "tags": [],
      "implicitDependencies": [
        "stencil"
      ]
    },
    "lanyon": {
      "tags": []
    },
    "lanyon-e2e": {
      "tags": [],
      "implicitDependencies": [
        "lanyon"
      ]
    },
    "docs": {
      "tags": []
    }
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/nx-cloud",
      "options": {
        "accessToken": "",
        "cacheableOperations": [
          //"build",
          "test",
          "lint"
        ],
        "canTrackAnalytics": false,
        "showUsageWarnings": true
      }
    }
  },
  "affected": {
    "defaultBase": "master"
  }
}
