{
  "npmScope": "nxext",
  "workspaceLayout": { "appsDir": "e2e", "libsDir": "packages" },
  "implicitDependencies": {
    "workspace.json": "*",
    "package.json": { "dependencies": "*", "devDependencies": "*" },
    "tslint.json": "*",
    "nx.json": "*",
    "tsconfig.base.json": "*",
    ".eslintrc.json": "*"
  },
  "projects": {
    "stencil": { "tags": [] },
    "stencil-e2e": { "tags": [], "implicitDependencies": ["stencil"] },
    "svelte": { "tags": [] },
    "svelte-e2e": { "tags": [], "implicitDependencies": ["svelte"] },
    "docs": { "tags": [] },
    "tailwind": { "tags": [] },
    "tailwind-e2e": { "tags": [], "implicitDependencies": ["tailwind"] },
    "platform": { "tags": [] },
    "stencil-compiler-utils": { "tags": [] }
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/nx-cloud",
      "options": {
        "accessToken": "",
        "cacheableOperations": ["build", "test", "lint"],
        "canTrackAnalytics": false,
        "showUsageWarnings": true,
        "scan": true
      }
    }
  },
  "affected": { "defaultBase": "main" }
}
